{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "build-storybook": {
      "dependsOn": ["prebuild"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "prebuild": {
      "outputs": ["dist/**"]
    },
    "storybook": {},
    "test": {},
    "test:ci": {
      "dependsOn": ["prebuild"]
    }
  }
}
